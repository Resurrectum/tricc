{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Debug Parser",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/diagram-parser/try_parser.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/diagram-parser/src"
            }
        },
        {
            "name": "Python: Debug Analysis",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/diagram-parser/debug_parser_new.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/diagram-parser/src"
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/diagram-parser/src"
            }
        },
        {
            "name": "Python: Run Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-v",
                "diagram-parser/tests"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/diagram-parser/src"
            }
        }
    ]
}